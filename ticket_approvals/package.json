{
  "name": "react_js-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "dev": "vite --port 3000",
    "watch": "vite build --watch --mode development",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build": "vite build --mode production",
    "lint": "eslint . --ext js,jsx --max-warnings 0",
    "clean": "node scripts/clean-dist.cjs",
    "verify:dist": "node scripts/verify-dist.cjs",
    "sync:app-id": "node scripts/sync-zcli-config.cjs",
    "validate": "npm run verify:dist && zcli apps:validate dist",
    "package:app": "npm run verify:dist && zcli apps:package dist",
    "deploy:create": "npm run clean && npm run build && npm run validate && zcli apps:create dist && npm run sync:app-id",
    "deploy:update": "npm run clean && npm run build && npm run validate && zcli apps:update dist",
    "release": "npm run deploy:update",
    "start": "zcli apps:server src",
    "start:prod": "zcli apps:server dist"
  },
  "dependencies": {
    "@zendeskgarden/css-bedrock": "^9.1.1",
    "@zendeskgarden/react-buttons": "^8.76.3",
    "@zendeskgarden/react-dropdowns": "^8.76.3",
    "@zendeskgarden/react-forms": "^8.76.3",
    "@zendeskgarden/react-grid": "^8.76.3",
    "@zendeskgarden/react-loaders": "^8.76.3",
    "@zendeskgarden/react-modals": "^8.76.3",
    "@zendeskgarden/react-notifications": "^8.76.3",
    "@zendeskgarden/react-tabs": "^8.76.3",
    "@zendeskgarden/react-theming": "^8.76.3",
    "@zendeskgarden/react-typography": "^8.76.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.1.0",
    "@testing-library/react": "^16.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.3.2",
    "jsdom": "^24.1.0",
    "prettier": "^3.2.5",
    "vite": "^6.3.5",
    "vitest": "^3.1.2"
  }
}
